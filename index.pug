link(rel='stylesheet', href='Estilo.css')
nav#header.navbar.navbar-expand-lg.navbar-expand-md.navbar-light.bg-light.sticky-top
  .container
    a.navbar-brand(href='index.html')
      img.rounded.logo(src='images/logo-fiat.png', alt='Logo Fiat')
    button.navbar-toggler(type='button', data-toggle='collapse', data-target='#navbarSupportedContent', aria-controls='navbarSupportedContent', aria-expanded='false', aria-label='Toggle navigation')
      span.navbar-toggler-icon
    .collapse.navbar-collapse.ms-navbar-collapse#navbarSupportedContent
      ul.navbar-nav.ml-auto
        li.nav-item.navbar-collapse
          a.nav-link#menu(href='#principal') MODELOS
        li.nav-item
          a.nav-link#menu1(href='#principal') PRUEBA DE MANEJO
        li.nav-item
          a.nav-link#menu2(href='#cotizar') COTIZAR
        li.nav-item
          a.nav-link#menu3(href='#principal') PROMOCIONES
        li.nav-item
          a.nav-link#menu4(href='#principal') DISTRIBUIDORES
section.principal#principal
  br
  .cotizador Cotizador
  .description Citadino, Aventurero, Deportivo
  .text-elegir Elige el tuyo
  .contenedor
    img.auto(src='images/fiat.png', alt='')
section.cotizar
  .formulario#cotizar
    .row.inicial
      .col-lg-6.col-md-6.col-ms-12.inicial
        form
          .form-group
            label.etiqueta(for='formGroupExampleInput') MODELO
            select.etiqueta.custom-select.custom-select-lg.mb-3.forma
              option(selected='') SELECCIONA
              option(value='1') MODELO 1
              option(value='2') MODELO 2
              option(value='3') MODELO 3
      .col-lg-6.col-md-6.col-ms-12.inicial
        form
          .form-group
            label.etiqueta(for='formGroupExampleInput') VERSION
            select.etiqueta.custom-select.custom-select-lg.mb-3.forma
              option(selected='') SELECCIONA
              option(value='1') VERSION 0.1
              option(value='2') VERSION 0.2
              option(value='3') VERSION 0.3
    .row
      .col-lg-6.col-md-6.col-ms-12
        form
          .form-group
            label.etiqueta(for='formGroupExampleInput') ESTADO
            select.etiqueta.custom-select.custom-select-lg.mb-3.forma
              option(selected='') SELECCIONA
              option(value='1') MEXICO
              option(value='2') JALISCO
              option(value='3') COAHUILA
      .col-lg-6.col-md-6.col-ms-12
        form
          .form-group
            label.etiqueta(for='formGroupExampleInput') DISTRIBUIDOR
            select.etiqueta.custom-select.custom-select-lg.mb-3.forma
              option(selected='') SELECCIONA
              option(value='1') DISTRIBUIDOR 1
              option(value='2') DISTRIBUIDOR 2
              option(value='3') DISTRIBUIDOR 3
    .row.mb-4
      .col-12.text-center
        span.datos.text-center Datos Personales
        p ESCRIBE TUS DATOS PERSONALES
    .row
      .col-lg-6.col-md-6.col-ms-12
        form
          .form-group
            label.etiqueta(for='formGroupExampleInput') NOMBRE (S)
            input.etiqueta.form-control.forma(type='text', placeholder='ESCRIBE TU NOMBRE')
      .col-lg-6.col-md-6.col-ms-12
        form
          .form-group
            label.etiqueta(for='formGroupExampleInput') APELLIDO PATERNO
            input.etiqueta.form-control.forma(type='text', placeholder='ESCRIBE TU APELLIDO')
      .col-lg-6.col-md-6.col-ms-12
        form
          .form-group
            label.etiqueta(for='formGroupExampleInput') APELLIDO MATERNO
            input.etiqueta.form-control.forma(type='text', placeholder='ESCRIBE TU APELLIDO')
    .row
      .col-lg-6.col-md-6.col-ms-12
        form
          .form-group
            label.etiqueta(for='formGroupExampleInput') EMAIL
            input.etiqueta.form-control.forma(type='email', placeholder='ESCRIBE TU CORREO')
      .col-lg-6.col-md-6.col-ms-12
        form
          .form-group
            label.etiqueta(for='formGroupExampleInput') TELEFONO
            input.etiqueta.form-control.forma(type='email', placeholder='TELEFONO')
    .row
      .col-lg-12.col-md-12.col-ms-12.text-center
        label.etiqueta(for='formGroupExampleInput') DESEO SER CONTACTADO POR
    .row
      .col-4.pr-0
        form
          .form-group
            input#bttelefono.etiqueta.bttext.form-control.forma.text-center.contacto(type='button', value=' üìû    TELEFONO')
      .col-4
        form
          .form-group
            input#btemail.etiqueta.form-control.forma.text-center.contacto(type='button', value='‚úâÔ∏è EMAIL')
      .col-4
        form
          .form-group
            input#btambos.etiqueta.form-control.forma.text-center.contacto(type='button', value='AMBOS')
    .row.mb-4.mt-4
      .col-12.text-center
        span.datos.text-center Forma de Pago
        p PERSONALIZA TU COTIZACION
    .row.pago
      .col-12.text-center
        form
          .form-group
            label.etiqueta(for='formGroupExampleInput') ELIGE TU FORMA DE PAGO
      .col-6
        form
          .form-group
            input#contado.etiqueta.form-control.forma.text-center.contacto(type='button', value=' CONTADO')
      .col-6
        form
          .form-group
            input#credito.etiqueta.form-control.forma.text-center.contacto(type='button', value='CREDITO')
    .row.pago
      .col-lg-6.col-md-6.col-ms-12
        form
          .form-group
            label.etiqueta(for='formGroupExampleInput') CANTIDAD DE ENGANCHE
            input.etiqueta.form-control.forma(type='text', placeholder='$ 80.000.00')
      .col-lg-6.col-md-6.col-ms-12
        form
          .form-group
            label.etiqueta.oculta(for='formGroupExampleInput') PLAZO
            select.etiqueta.custom-select.custom-select-lg.mb-3.forma
              option(selected='') SELECCIONA
              option(value='1') 12 MESES
              option(value='2') 24 MESES
              option(value='3') 48 MESES
              option(value='3') 72 MESES
    .row.mb-1.mt-4
      .col-12.text-center
        span.datos.text-center Comentarios
        p NOS IMPORTAN TUS COMENTARIOS
    .row
      .col
        form
          .form-group
            label.etiqueta(for='formGroupExampleInput') ¬øDUDAS? ¬øCOMENTARIOS?
            textarea.form-control.comentaios.forma(rows='8', placeholder='ESCRIBENOS')
    .terminos.mt-4
      .form-check
        input.form-check-input(type='checkbox')
        label.form-check-label.etiqueta.font(for='exampleCheck1') He leido y acepto el aviso de privacidad
      .form-check.mt-3
        input.form-check-input(type='checkbox')
        label.form-check-label.etiqueta.font(for='exampleCheck1') No deseo recibir notificaciones
        button.btenvio.forma.etiqueta#btenviar ENVIAR
    .final
      a(href='#')
        p @Fiat2019
      p Todos los derechos reservados
